WHITESPACE = _{ " " | "\t" | "\r" | "\n" }

xml = { SOI ~ element ~ EOI }

element = { start_tag ~ (element | text)* ~ end_tag }
start_tag = { "<" ~ name ~ (WHITESPACE ~ attribute)* ~ WHITESPACE? ~ ">" }
end_tag = { "</" ~ name ~ ">" }

attribute = { name ~ "=" ~ "\"" ~ value ~ "\"" }
name = { (ASCII_ALPHANUMERIC | "-" | "_" | ":")+ }
value = { (!"\"" ~ ANY)* }

text = { (!"<" ~ ANY)+ }